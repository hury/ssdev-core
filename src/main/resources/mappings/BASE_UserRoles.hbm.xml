<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="ctd.account.user.UserRoleTokenEntity" table="BASE_UserRoles">
			<id name="id" type="java.lang.Integer">
            	<column name="id"/>
            	<generator class="identity" />
        	</id>
        	<property name="userId" type="java.lang.String" length="20" />
        	<property name="roleId" type="java.lang.String" length="50" />
        	<property name="tenantId" type="java.lang.String" length="50" />
        	<property name="manageUnit" type="java.lang.String" length="50" />
        	<property name="lastLoginTime" type="java.util.Date"  />
        	<property name="lastIPAddress" type="java.lang.String" length="18" />
</class>
</hibernate-mapping>